def GCD(a,b):
    if b!=0 and a!=0:
        print('a='+str(a)+' b='+str(b),end='')
        r = a%b
        print(" => r="+str(r))
        if r!=0:
            return GCD(b,r)
        
        else:
            print('a='+str(a),'r='+str(r))
            return a
    elif a==0:
        return b
    elif b==0:   
        return a
    else:
        print("Error 101!")
GCD(24,14)
